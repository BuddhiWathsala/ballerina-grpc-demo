import ballerina/grpc;
import ballerina/log;

listener grpc:Listener ep = new (9091);

service TravelGuide on ep {

    resource function GetRoute(grpc:Caller caller) {
        log:printInfo("Invoke GetRoute resource.");
        Location[] route = [
            {name: "Colombo", latitude: 85, longitude: 100},
            {name: "Mumbai", latitude: 72, longitude: 82},
            {name: "London", latitude: 30, longitude: 62}
        ];
        foreach Location location in route {
            grpc:Error? err = caller->send(location);
            if (err is grpc:Error) {
                log:printError("Error from Connector: " + err.message());
            } else {
                log:printInfo("Send location: " + location.name);
            }
        }

        grpc:Error? result = caller->complete();
        if (result is grpc:Error) {
            log:printError("Error in sending completed notification to caller",
                err = result);
        }
    }
}

public type Empty record {|
    
|};

public type Location record {|
    string name = "";
    int latitude = 0;
    int longitude = 0;
    
|};



const string ROOT_DESCRIPTOR = "";
function getDescriptorMap() returns map<string> {
    return {
        "s_streaming.proto":"",
        "google/protobuf/empty.proto":""
        
    };
}

