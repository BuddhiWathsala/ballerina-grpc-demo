import ballerina/grpc;

public type TravelGuideClient client object {

    *grpc:AbstractClientEndpoint;

    private grpc:Client grpcClient;

    public function init(string url, grpc:ClientConfiguration? config = ()) {
        // initialize client endpoint.
        self.grpcClient = new(url, config);
        checkpanic self.grpcClient.initStub(self, "non-blocking", ROOT_DESCRIPTOR, getDescriptorMap());
    }

    public remote function SendRoute(service msgListener, grpc:Headers? headers = ()) returns (grpc:StreamingClient|grpc:Error) {
        return self.grpcClient->streamingExecute("TravelGuide/SendRoute", msgListener, headers);
    }
};

public type Empty record {|
    
|};


public type Location record {|
    string name = "";
    int latitude = 0;
    int longitude = 0;
    
|};



const string ROOT_DESCRIPTOR = "";
function getDescriptorMap() returns map<string> {
    return {
        "c_streaming.proto":"",
        "google/protobuf/empty.proto":""
        
    };
}

